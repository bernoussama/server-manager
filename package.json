{
  "name": "server-manager",
  "version": "1.0.0",
  "description": "A web application for managing servers",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "pnpm start:backend",
    "start:backend": "pnpm --filter backend start",
    "start:ui": "pnpm --filter ui preview",
    "preview": "pnpm --filter ui preview",
    "dev:shared": "pnpm --filter @server-manager/shared dev",
    "dev:backend": "pnpm --filter backend dev",
    "dev:ui": "pnpm --filter ui dev",
    "dev": "turbo run dev",
    "clean": "pnpm -r exec -- rm -rf dist node_modules"
  },
  "author": "bernoussama",
  "license": "GPL-3.0-only",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "concurrently": "^9.1.2",
    "esbuild-fix-imports-plugin": "^1.0.21",
    "tsup": "^8.5.0",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "server",
    "manager",
    "ui",
    "linux",
    "server-manager",
    "server-manager-ui",
    "devops",
    "server-management",
    "server-monitoring",
    "server-administration",
    "server-management-ui",
    "administration",
    "management",
    "monitoring",
    "administration",
    "management",
    "monitoring",
    "administration",
    "management",
    "monitoring"
  ]
}