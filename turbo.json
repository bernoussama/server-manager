{
    "$schema": "https://turborepo.com/schema.json",
        "globalDependencies": [
          "tsup.config.ts",
          "package.json",
          "pnpm-lock.yaml",
          "pnpm-workspace.yaml",
          "tsup.config.ts",
          "tsconfig.json",
          "turbo.json"
      ],
    "tasks": {
      "build": {
        "outputs": ["dist/**"]
      },
      "check-types": {
        "dependsOn": ["^check-types"]
      },
      "dev": {
        "cache": false
      },
  
          "generate": {
              "dependsOn": ["^generate"],
              "cache": false
          },
          "lint": {},
          "test": {}
      }
  }