{
    "$schema": "https://turborepo.com/schema.json",
        "globalDependencies": [
          "package.json",
          "pnpm-lock.yaml",
          "pnpm-workspace.yaml",
          "tsconfig.json",
          "turbo.json"
      ],
    "tasks": {
      "build": {
        "outputs": ["dist/**"]
      },
      "check-types": {
        "dependsOn": ["^check-types"]
      },
      "dev": {
        "cache": false
      },
      "generate": {
          "dependsOn": ["^generate"],
          "cache": false
      },
      "lint": {},
      "test": {}
      }
  }